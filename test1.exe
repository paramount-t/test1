#include<iostream>
using namespace std;
 int check(int n)
 {
 	while(n!=0)
 	{
 		if(n%10==2||n%10==4)
 		return 0;
		n/=10;
	 }
	 return 1; 
 }
int main()
{
	int d=0;
	int a,b,c;
	int x;
	cin>>x;
	for(a=1;a<(x-1)/2;a++)
	{
		for(b=a+1;b<(x-1)/2;b++)
		{
			for(c=b+1;c<x;c++)
			if(a+b+c==x&&check(a)&&check(b)&&check(c))
			{	
			cout<<a<<"  "<<b<<"  "<<c<<endl;
			d++;
		}
}
}
	cout<<endl<<d;
	return 0;
}

